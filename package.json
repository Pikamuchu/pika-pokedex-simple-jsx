{
  "name": "simple-jsx-examples",
  "version": "1.0.0",
  "description": "Simple-jsx usage examples",
  "main": "./src/index.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build:client": "webpack --progress --hide-modules --mode production",
    "build:server": "babel src -d dist",
    "build": "npm run build:client && npm run build:server",
    "watch": "webpack --watch --mode development",
    "start": "nodemon --exec babel-node src/server/index.js",
    "test": "nyc mocha --require @babel/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pikamachu/pika-searchquire-examples.git"
  },
  "keywords": [
    "express",
    "simple-jsx",
    "web"
  ],
  "author": "Antonio Marin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pikamachu/pika-searchquire-examples/issues"
  },
  "homepage": "https://github.com/pikamachu/pika-searchquire-examples#readme",
  "dependencies": {
    "express": "^4.16.3",
    "simple-jsx": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/runtime": "^7.4.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-simple-jsx": "^1.0.1",
    "chai": "^4.2.0",
    "chai-subset": "^1.5.0",
    "codecov": "^3.5.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "nyc": "^14.1.1",
    "prettier": "^1.13.0",
    "searchquire": "^1.5.0",
    "sinon": "^7.1.1",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  }
}
